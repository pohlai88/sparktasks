# Ignore artifacts:
dist
coverage
node_modules

# Ignore config files (they have their own formatting)
*.config.js
*.config.ts

# Ignore generated files
*.d.ts
!src/**/*.d.ts

# Ignore package files
package-lock.json
yarn.lock
pnpm-lock.yaml

# Ignore logs
*.log

# OS generated files
.DS_Store
Thumbs.db
