{
  "extends": ["../.eslintrc.cjs"],
  "parserOptions": {
    "project": "./tsconfig.json",
    "tsconfigRootDir": "__dirname"
  },
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_" }
    ],
    "@typescript-eslint/no-explicit-any": "warn",
    "no-console": "off"
  },
  "ignorePatterns": ["node_modules/", "dist/", "build/", "test-results/"]
}
